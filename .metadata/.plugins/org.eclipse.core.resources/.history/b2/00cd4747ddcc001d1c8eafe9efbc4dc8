package ejercicio2;



public class ejercicio2 {

	public static int[] makeArray(int num){
		  int[] newArray = new int[num];
		  for(int i=1; i<num; i++) {
			  newArray[i-1] = num*i;
		  }
		  return newArray;
		
	}
	
	public static void main(String[] args) {
		
		int[] unArray = makeArray(5);
		System.out.println(unArray);

	}

}
