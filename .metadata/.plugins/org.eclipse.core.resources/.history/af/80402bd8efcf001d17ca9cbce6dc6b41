package ejercicio4;


public class SwapValores {

	public static void swap1 (int x, int y) {
		if(x < y) {
			int tmp = x;
			x = y;
			y = tmp;
		}
		System.out.println(x+" "+y);
	}
	
	public static void swap2 (Integer x, Integer y) {
		if(x < y) {
			int tmp = x;
			x = y;
			y = tmp;
		}
		System.out.println(x+" "+y);
	}
	
	public static void main(String[] args) {
		
		int x1 = 5;
		int y1 = 8;
		Integer x2 = 5;
		Integer y2 = 8;
		swap1(x1,y1);
		System.out.println(x1+""+"y2");
		swap2(x2,y2);
		

	}

}
