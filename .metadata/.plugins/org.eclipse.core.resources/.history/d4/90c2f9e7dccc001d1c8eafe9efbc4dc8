package ejercicio2;



public class ejercicio2 {

	public int[] ejercicio2(int num){
		  int[] newArray = new int[num];
		  for(int i=1; i<num; i++) {
			  newArray[i-1] = num*i;
		  }
		  return newArray;
		
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
