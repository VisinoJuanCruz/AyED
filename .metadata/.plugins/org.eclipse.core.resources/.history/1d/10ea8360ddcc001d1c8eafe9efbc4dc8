package ejercicio2;



public class ejercicio2 {

	public static int[] makeArray(int num){
		  int[] newArray = new int[num];
		  for(int i=0; i<num; i++) {
			  newArray[i] = num*(i+1);
		  }
		  return newArray;
		
	}
	
	public static void main(String[] args) {
		
		int[] unArray = makeArray(5);
		for(int i=0; i<unArray.length;i++) {
			System.out.println(unArray[i]);
		}

	}

}
